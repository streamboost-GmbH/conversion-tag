___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// Vorlagencode hier eingeben.
const logToConsole = require('logToConsole');
const queryPermission = require('queryPermission');
const sendPixel = require('sendPixel');
const encodeUri = require('encodeUri');

const readTitle = require('readTitle');
const getUrl = require('getUrl');

// Debug Log wenn gewünscht
const log = data.debug ? logToConsole : (() => {});

// Streamboost API URL
const url = 'https://api.streamboost.de/api/trackparam';
log('Using base URL '+url);

const advertiserId = data.advertiser_id;
const trackId = data.campaign_tracking_id;
log('Advertiser id: '+advertiserId);
log('Track id: '+trackId);

// Titel der Seite erfragen
let title = 'UNKNOWN';
if (queryPermission('read_title')) {
  title = readTitle();
}

// Aktuelle URL der Seite erfragen
let ownURL = 'UNKNOWN';
if (queryPermission('get_url')) {
  ownURL = getUrl();
}

const buildUrl = url+'?advertiser_id='+advertiserId+'&track_id='+trackId+'&title='+title+'&own_url='+ownURL;
const encodedURL = encodeUri(buildUrl);

log('Complete URL: '+encodedURL);

if (queryPermission('send_pixel', buildUrl)) {
  log('Permission check: OK');
  sendPixel(encodedURL, data.gtmOnSuccess(), data.gtmOnFailure);
} else {
  log('Permission check: FAIL');
  data.gtmOnFailure();
}


